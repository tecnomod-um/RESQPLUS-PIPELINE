@prefix fno: <https://w3id.org/function/ontology#> .
@prefix fnom: <https://w3id.org/function/vocabulary/mapping#> .
@prefix ex: <http://example.org/functions#> .

ex:addTemporalContext a fno:Function ;
    fno:name "addTemporalContext" ;
    fno:expects ( [ fno:predicate fno:input ] ) ;
    fno:returns ( [ fno:predicate fno:output ] ) ;
    fnom:implementation [
        a fnom:WebService ;
        fnom:endpoint "http://localhost:5000/addTemporalContext" ;
        fnom:hasRequestMethod "POST"
    ].


